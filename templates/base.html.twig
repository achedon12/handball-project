<!DOCTYPE html>
<html>
    <head >
        <meta charset="UTF-8">
        <title>{% block title %}{% endblock %}</title>
        <link rel="stylesheet" href="{{ asset('css/style.css') }}">
        <link rel="stylesheet" href="{{ asset('css/goTopButton.css') }}">
        <link rel="stylesheet" type="text/css" href="{{asset('css/bootstrap.min.css')}}" />
        <link rel="stylesheet" type="text/css" href="{{asset('css/bootstrap-icons.css')}}" />
        <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css">
        <link rel="icon" href="{{ asset('images/logo.png') }}">
        {# Run `composer require symfony/webpack-encore-bundle` to start using Symfony UX #}
        {% block stylesheets %}
            {{ encore_entry_link_tags('app') }}

        {% endblock %}
        {% block sidebar %}
            {{ encore_entry_link_tags('app') }}
        {% endblock %}

        {% block javascripts %}
            {{ encore_entry_script_tags('app') }}
        {% endblock %}

    </head >
    <body>
    <button onclick="topFunction()" id="myBtn" title="Go to top"><img src="{{ asset('images/gotop.png') }}" alt=""></button>
        {% block body %}{% endblock %}
        {% block divMain %}{% endblock %}
    </body>
    <script src="{{ asset('js/goTopButton.js') }}"></script>
</html>
